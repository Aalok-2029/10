// Factor
#include<stdio.h>
int main()
    {
        int n,i;
        printf("Enter a number : ");
        scanf("%d",&n);
        printf("Factor's of %d are : ");
        for(i=1;i<=n;i++)
            {
                if(n%i==0)
                    {
                        printf("%d\t",i);
                    }	
            }
    }
// Factorcial
  #include<stdio.h>
int main()
    {
        int n,i,f=1;
        printf("Enter a number : ");
        scanf("%d",&n);
        for(i=n;i>=1;i--)
            {
                f=f*i;
            }
            printf("Factorical of %d is %d",n,f);
    }
// Multiplication Table
  printf("Multiplication Table of %d are :",n);    
  for(i=1;i<=n;i++)
        {
            printf("%d x %d = %d",n,i,n*i);
        }
