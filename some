// Factor
#include<stdio.h>
int main()
    {
        int n,i;
        printf("Enter a number : ");
        scanf("%d",&n);
        printf("Factor's of %d are : ");
        for(i=1;i<=n;i++)
            {
                if(n%i==0)
                    {
                        printf("%d\t",i);
                    }	
            }
    }
// Factorcial
  #include<stdio.h>
int main()
    {
        int n,i,f=1;
        printf("Enter a number : ");
        scanf("%d",&n);
        for(i=n;i>=1;i--)
            {
                f=f*i;
            }
            printf("Factorical of %d is %d",n,f);
    }
// Multiplication Table
  printf("Multiplication Table of %d are :",n);    
  for(i=1;i<=n;i++)
        {
            printf("%d x %d = %d",n,i,n*i);
        }
// reverse of number
  #include<stdio.h>
 int main()
    {
        int n,o,r,R=0;
        printf("Enter a number : ");
        scanf("%d",&n);
        o=n;
        while(n>0)
            {
                r=n%10;
                R=R*10+r;
                n=n/10;
            }
            printf("Reverse of %d is %d",o,R);
    }













